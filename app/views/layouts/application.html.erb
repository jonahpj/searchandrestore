<!DOCTYPE html>
<html>
<head>
  <title>Searchandrestore</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<% if flash[:notice] %>
  <div class="flash" id="notice">
    <%= flash[:notice] %>
  </div>
<% end %>

<% if flash[:alert] %>
  <div class="flash" id="alert">
    <%= flash[:alert] %>
  </div>
<% end %>

<%= yield %>

</body>
</html>
